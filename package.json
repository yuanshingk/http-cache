{
  "name": "http-cache",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev\" \"npm run pug-watch\" \"npm run ui\"",
    "serve": "nodemon --inspect=0.0.0.0:30003 ./src/index.js",
    "ui": "browser-sync start --config bs-config.js",
    "pug-watch": "pug -w ./src/views -o ./src/public -P",
    "pug-output": "pug ./src/views -o ./src/public -P"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pug": "^3.0.2"
  }
}
